{
  "name": "my-notes",
  "version": "1.0.0",
  "description": "my-notes exposes CRUD APIs required for notes management",
  "main": "index.js",
  "scripts": {
    "build": "npm i && tsoa spec-and-routes && tsc",
    "start-dev": "NODE_ENV=test nodemon -e ts -w ./src -x ts-node src/index.ts",
    "start": "NODE_ENV=test ts-node src/index.ts",
    "prettier": "prettier --check \"**/*\"",
    "prettier-fix": "prettier --write '**/*.js' '**/*.json' '**/*.ts' '**/*.md'"
  },
  "author": "Jay",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/pino": "^7.0.5",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "config": "^3.3.11",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.2.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1"
  }
